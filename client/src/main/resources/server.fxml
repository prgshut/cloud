<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox  xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PanelServer">
   <children>
                  <HBox >
                     <children>
                        <ComboBox fx:id="cbDiskServer" />
                         <Button fx:id="update" onAction="#update" text="update"/>
                        <TextField fx:id="textServer"  HBox.hgrow="ALWAYS" />
                        <Button fx:id="upServer"  onAction="#upServer" mnemonicParsing="false" text="up" />
                     </children>
                  </HBox>
                  <TableView fx:id="tvServer"  VBox.vgrow="ALWAYS"/>
   </children>
    <properties>
        <ctrl><fx:reference source="controller"/></ctrl>
    </properties>
</VBox>
